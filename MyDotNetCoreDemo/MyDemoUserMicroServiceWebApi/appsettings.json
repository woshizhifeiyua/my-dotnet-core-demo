{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConsulRegist": { //consul服务注册
    "IsRegist": true, //是否注册
    "RegistMode": 0, //0  配置文件注册 1 命令行注册 如果是命令行注册 则需要将后面全部输入在命令行中
    "Address": "http://localhost:8500/",
    "Datacenter": "dcl",
    "Id": "UserService", //id前缀
    "Name": "UserService",
    "ip": "127.0.0.1",
    "port": "5000",
    "weight": "1", //权重
    "AgentServiceCheck": {
      "Interval": 10,
      "HTTP": "/Api/WeatherForecast/index",
      "Timeout": 6, //秒
      "DeregisterCriticalServiceAfter": 5 //错误后多久去掉服务
    }
  },
  "Swagger": { //服务注册
    "IsRegist": true, //是否注册
    "Name": "UserService",
    "Title": "用户接口文档",
    "Version": "V1",
    "Description": "用户接口文档"
  }
}
